pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
      stage('Build'){
        steps{
          echo 'building project'
       }
      }
      stage('Test'){
        steps{
          echo 'testing project'
        }
      }
      stage('deploy'){
        steps{
          echo 'deploying project'
      }
        
       }
}
}
